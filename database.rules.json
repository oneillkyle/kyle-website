{
  "rules": {
    ".read": false,
    ".write": false,
    "posts": {
      ".read": true,
      ".write": "root.child('groups/admin').hasChild(auth.uid)",
      ".indexOn": "inverseDate"
    },
    "about": {
      ".read": true,
      ".write": "root.child('groups/admin').hasChild(auth.uid)"
    },
    "groups": {
      ".read": "root.child('groups/admin').hasChild(auth.uid)",
      ".write": false
    }
  }
}
