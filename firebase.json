{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/node_modules/**",
      "**/jspm_packages/**"
    ]
  }
}
