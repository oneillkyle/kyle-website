{
  "name": "angular2-site",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" \"jspm bundle app public/app.js -wid\"",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^1.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-injector": "^1.0.0",
    "grunt-processhtml": "^0.3.13",
    "grunt-shell": "^1.3.0",
    "jspm": "^0.17.0-beta.14",
    "lite-server": "^2.0.1",
    "typings": "^0.6.8"
  },
  "jspm": {
    "main": "angular2-site.js",
    "directories": {
      "lib": "app"
    },
    "dependencies": {
      "@angular/common": "npm:@angular/common@2.0.0-rc.1",
      "@angular/compiler": "npm:@angular/compiler@2.0.0-rc.1",
      "@angular/core": "npm:@angular/core@2.0.0-rc.1",
      "@angular/http": "npm:@angular/http@2.0.0-rc.1",
      "@angular/platform-browser": "npm:@angular/platform-browser@2.0.0-rc.1",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@2.0.0-rc.1",
      "@angular/router": "npm:@angular/router@2.0.0-rc.1",
      "@angular/router-deprecated": "npm:@angular/router-deprecated@2.0.0-rc.1",
      "@angular/upgrade": "npm:@angular/upgrade@2.0.0-rc.1",
      "angular2-in-memory-web-api": "npm:angular2-in-memory-web-api@0.0.7",
      "babel": "npm:babel@^6.5.2",
      "bootstrap": "npm:bootstrap@^3.3.6",
      "css": "github:systemjs/plugin-css@^0.1.21",
      "es6-promise": "npm:es6-promise@^3.0.2",
      "es6-shim": "npm:es6-shim@^0.33.3",
      "jquery": "npm:jquery@^2.2.3",
      "lodash": "npm:lodash@^4.8.2",
      "materialize-css": "npm:materialize-css@^0.97.6",
      "ng2-ckeditor": "github:chymz/ng2-ckeditor@^1.0.3",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.10",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3",
      "rxjs": "npm:rxjs@5.0.0-beta.6",
      "scss": "github:mobilexag/plugin-sass@^0.4.3",
      "systemjs": "npm:systemjs@^0.19.20",
      "tether": "npm:tether@^1.2.4",
      "zone.js": "npm:zone.js@^0.6.12"
    },
    "devDependencies": {
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.10",
      "typescript": "npm:typescript@^1.7.5"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "github:components/jquery@2.2.1": {
        "main": "jquery"
      },
      "npm:@angular/common@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/http@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/router@2.0.0-rc.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:angular2@2.0.0-beta.17": {
        "main": false,
        "dependencies": {
          "reflect-metadata": "npm:reflect-metadata@0.1.2",
          "rxjs": "npm:rxjs@^5.0.0-beta.2",
          "zone.js": "npm:zone.js@^0.6.6"
        },
        "peerDependencies": {
          "reflect-metadata": "npm:reflect-metadata@0.1.2",
          "rxjs": "npm:rxjs@^5.0.0-beta.2",
          "zone.js": "npm:zone.js@^0.6.6"
        },
        "jspmNodeConversion": false,
        "map": {
          "crypto": "@empty"
        },
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.3": {
        "format": "amd"
      },
      "npm:materialize-css@0.97.6": {
        "format": "global",
        "main": "dist/js/materialize",
        "dependencies": {
          "jquery": "github:components/jquery",
          "css": "github:systemjs/plugin-css"
        },
        "jspmNodeConversion": false,
        "meta": {
          "dist/js/materialize.js": {
            "deps": [
              "jquery",
              "../css/materialize.css!"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:reflect-metadata@0.1.3": {
        "jspmNodeConversion": true,
        "map": {}
      },
      "npm:sass.js@0.9.10": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
